<style>
    #spl_timetable {
        font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

    #spl_timetable td, #spl_timetable th {
        border: 1px solid darkgrey;
        padding: 8px;
    }

    #spl_timetable button{
        background-color: green;
        color: whitesmoke;
        height: 30px;
        border-radius: 30px;
        padding: 5px 5px 5px 4px;
        font-weight: bolder;
        width: 100%;
    }

    #spl_timetable b{
        text-align: center;
    }

    #spl_timetable tr:nth-child(even){background-color: #f2f2f2;}

    #spl_timetable tr:hover {background-color: #ddd;}

    #spl_timetable th {
        padding-top: 12px;
        padding-bottom: 12px;
        background-color: #4CAF50;
        color: white;
        text-align: center!important;
    }
</style>


